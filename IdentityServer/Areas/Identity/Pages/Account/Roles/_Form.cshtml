

@model IdentityServer.Areas.Identity.Pages.Account.Roles.NewModel


<div class="card card-primary">
  <div class="card-header">
    <h3 class="card-title">Datos</h3>
  </div>
  <div class="card-body">
     <form id="rolesForm"  method="post">
        <div asp-validation-summary="ModelOnly" class="text-danger"></div>
        <div class="form-floating">
            <input asp-for="Input.Name" class="form-control" autocomplete="name" aria-required="true" />
            <label asp-for="Input.Name"></label>
            <span asp-validation-for="Input.Name" class="text-danger"></span>
        </div>
        <div class="form-floating">
            <input asp-for="Input.NormalizedName" class="form-control" autocomplete="normalizedName" aria-required="true" />
            <label asp-for="Input.NormalizedName"></label>
            <span asp-validation-for="Input.NormalizedName" class="text-danger"></span>
        </div>
        <div class="card-footer">
            <div class="actions">
                <button id="registerSubmit" type="submit" class="btn btn-primary">Registrar</button>
            </div>
        </div>

     </form>
   </div>
</div>

